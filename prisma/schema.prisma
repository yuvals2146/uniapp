// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PoolInfo {
  id          Int  @id @default(autoincrement())
  createdAt   DateTime @default(now())
  feesToken0  Float     
  feesToken1  Float
  priceToken0 Float 
  priceToken1 Float 
  pair        String
  liquidity   Float
  poolId      Int?

}
